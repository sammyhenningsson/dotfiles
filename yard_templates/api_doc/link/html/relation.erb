<h4 class=description-list>
    <% sources.each do |source:, class_name:| %>
        <span class="property-source <%= class_name %>"><%= source %></span>
    <% end %>
    <strong> <%= object.name %></strong>
</h4>
<p><%= object.documentation %></p>

<% if href %>
    <p>Reference: <a href="<%= href %>"><%= href %></a></p>
<% end %>

<% unless http_methods.empty? %>
    <p>Supported HTTP Methods: <%= http_methods %></p>
<% end %>

<% if content_type %>
    <p>Response type: <%= content_type %></p>
<% end %>
